{
  "name": "typescript-boilerplate",
  "version": "3.0.0",
  "description": "[Project Name] description",
  "main": "./src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts --trace-warnings",
    "test": "jest --updateSnapshot --detectOpenHandles --coverage --forceExit --verbose",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write .",
    "dev": "nodemon --watch '*.ts' --exec 'ts-node' src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amin-abbasi/typescript-boilerplate.git"
  },
  "keywords": [
    "typescript",
    "boilerplate"
  ],
  "author": "AAH",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amin-abbasi/typescript-boilerplate/issues"
  },
  "homepage": "https://github.com/amin-abbasi/typescript-boilerplate.git#readme",
  "dependencies": {
    "dotenv": "17.3.1",
    "express": "5.2.1",
    "express-rate-limit": "8.2.1",
    "gach": "1.7.5",
    "helmet": "8.1.0",
    "joi": "18.0.2",
    "jsonwebtoken": "9.0.3",
    "mongoose": "9.2.1",
    "mongoose-unique-validator": "5.0.2",
    "mysql": "2.18.1",
    "redis": "5.11.0",
    "reflect-metadata": "0.2.2",
    "socket.io": "4.8.3",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "typeorm": "0.3.28",
    "undici": "7.22.0"
  },
  "devDependencies": {
    "@types/express": "5.0.6",
    "@types/helmet": "4.0.0",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "25.2.3",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.8",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "jest": "30.2.0",
    "nodemon": "3.1.11",
    "prettier": "3.8.1",
